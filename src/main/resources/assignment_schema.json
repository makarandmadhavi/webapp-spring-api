{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "points": {
      "type": "integer"
    },
    "num_of_attempts": {
      "type": "integer"
    },
    "deadline": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [ "name", "points", "num_of_attempts", "deadline"],
  "additionalProperties": false
}
